SOLUTION
    case getModes
        eigen
    case invMat
        directfrf-materialid
END
OPTIMIZATION
    ROLmethod = linesearch
    LSstep = newton-krylov
    //want to try with and w/o following parameters:
    //Max_iter_Krylov = 50
    opt_tolerance = 1e-8
    opt_iterations = 5
    scaleDesignVars = yes
END
INVERSE-PROBLEM
    data_truth_table = ttable.txt
    real_data_file = dataReal.txt
    imaginary_data_file = dataImag.txt
    data_type = disp
END
FILE
    geometry_file = 'expmodel_3.e'
END
FREQUENCY
    freq_min = 15
    freq_max = 15
    freq_step = 10
    block all
    disp
END
BOUNDARY
    nodeset 1 
        fixed
END
LOADS
	nodeset 2
	    pressure = 1e6
	    function 1
END
FUNCTION 1
    type linear
    data 0 0
    data 0 1
END
BLOCK 1
    material 1
    inverse_material_type = heterogeneous
END
MATERIAL 1
    G_bounds = 1 1e5
    K_bounds = 1 1e5
    G 1e4
    K 1e4
    //E 2200e6
    //nu 0.35
    density 1040
END
OUTPUTS
    disp
    stress
END
ECHO
END
