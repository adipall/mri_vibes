FEMesh.icosahedron;
elementType = 'Tri3';
fexo = FEMesh.Exodus( elementType, obj.connectivity, obj.coordinates );
fexo.Title = 'Icosahedron';
fexo.Filename = 'sphere.g';
exo_put(filename,fexo);
